    "import socket \n",
    "import  os\n",
    "s=socket.socket(socket.AF_INET , socket.SOCK_DGRAM)\n",
    "ip='192.168.43.211'\n",
    "port = 5678\n",
    "s.bind((ip,port))\n",
    "\n",
    "\n",
    "while True :\n",
    "    x=s.recvfrom(1024)\n",
    "    y=x[0]\n",
    "    print(y.decode())\n",
    "    send=input()\n",
    "    s.sendto(send.encode(),(\"192.168.43.136\",1234))\n",
    "\n",
